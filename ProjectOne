import numpy as np
import json

resistances = json.loads(''')
voltages = json.loads(''')

A = np.zeros(25,25)
b = np.zeros(25)

b[0] = 10.0
b[24] = 0.0

for resistance in resistances:
    n1 = resistance['node1'] - 1  # Convert to zero index
    n2 = resistance['node2'] - 1  # Convert to zero index
    r = resistance['resistance']
    A[n1][n1] += 1 / r
    A[n1][n2] -= 1 / r
    A[n2][n2] += 1 / r
    A[n2][n1] -= 1 / r

print("Matrix A:")
print(A)
print("\nVector b:")
print(b)
